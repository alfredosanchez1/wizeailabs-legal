{
  "version": 2,
  "name": "wizeailabs-legal",
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "styles/**/*.css",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/recipetuner/privacy-policy",
      "dest": "/recipetuner/privacy-policy.html"
    },
    {
      "src": "/recipetuner/terms-of-service",
      "dest": "/recipetuner/terms-of-service.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
